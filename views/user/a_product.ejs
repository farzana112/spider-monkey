<div class="product-details-area pb-100 pt-100">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="product-details-img-wrap product-details-vertical-wrap" data-aos="fade-up" data-aos-delay="200">
                    <div class="product-details-small-img-wrap">
                        <div class="swiper-container product-details-small-img-slider-1 pd-small-img-style">
                            <div class="swiper-wrapper">
                              <%for(var i =0 ; i<oneProduct.images.length;i++){%>
                                <div class="swiper-slide">
                                    <div class="product-details-small-img">
                                        <img src="\<%= oneProduct.images[i].path%>" alt="Product Thumnail">
                                    </div>
                                </div>
                                <%}%>
                                
                            </div>
                        </div>
                        <div class="pd-prev pd-nav-style"> <i class="ti-angle-up"></i></div>
                        <div class="pd-next pd-nav-style"> <i class="ti-angle-down"></i></div>
                    </div>
                    <div class="swiper-container product-details-big-img-slider-1 pd-big-img-style">
                        <div class="swiper-wrapper">
                            <%for(var i =0 ; i<oneProduct.images.length;i++){%>
                            <div class="swiper-slide">
                                <div class="easyzoom-style">
                                    <div class="easyzoom easyzoom--overlay">
                                        <a href="\<%= oneProduct.images[i].path%>">
                                            <img src="\<%= oneProduct.images[i].path%>" alt="">
                                        </a>
                                    </div>
                                    <a class="easyzoom-pop-up img-popup" href="\<%= oneProduct.images[i].path%>">
                                        <i class="pe-7s-search"></i>
                                    </a>
                                </div>
                            </div>
                            <%}%>
                            
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
            <form action="/products/addtocart/<%= oneProduct.id%>" method="POST" id="addtocart">
                <div class="product-details-content" data-aos="fade-up" data-aos-delay="400">
                    <h2><%= oneProduct.itemName%></h2>
                    <div class="product-details-price">
                        <input type="text" name="price" style="display: none;" value="<%= oneProduct.price%>">
                        <input type="text" name="prodId" style="display: none;" value="<%= oneProduct._id%>">
                        <span class="new-price">â‚¹ <%= oneProduct.price%></span>
                    </div>
                    <div class="product-details-action-wrap">
                         <div class="product-quality">
                            <input class="cart-plus-minus-box input-text qty text" name="quantity" value="1">
                            

                        </div> 
                         <div class="single-product-cart btn-hover">
                            <a href="#" onclick="document.getElementById('addtocart').submit();">Add to cart</a>
                        </div>
                       
                    </form>
                        <div class="single-product-wishlist">
                            <form action="/add_to_wishlist/<%=oneProduct.id%>" method="POST">
                            <!-- <a title="Wishlist" href="wishlist.html"><i class="pe-7s-like"></i></a> -->
                            <button type="submit"class="product-action-btn-2" title="Wishlist"><i class="pe-7s-like"></i></button>
                        </form>
                        </div>
                        
                    </div>
                    <div class="product-details-meta">
                       <strong> Category:</strong><%=oneProduct.category%><br>
                       <strong> Description:</strong><%= oneProduct.description%>
                    </div>
                    
                </div>
            </div>
        </div>
    </div>
</div>
